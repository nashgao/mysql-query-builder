{
    "name": "nashgao/mysql-query-builder",
    "type": "library",
    "license": "MIT",
    "description": "self used mysql encapsulation for hyperf framework",
    "authors": [
        {
            "name": "nashgao",
            "email": "nashgao1992@gmail.com"
        }
    ],
    "require": {
        "hyperf/database": "2.2.*@beta",
        "hyperf/utils": "2.2.*@beta",
        "hyperf/model-cache": "2.2.*@beta",
        "myclabs/deep-copy": "^1.10",
        "nashgao/utils": "dev-master",
        "hyperf/db-connection": "2.2.*@beta",
        "hyperf/paginator": "2.2.*@beta",
        "hyperf/snowflake": "2.2.*@beta",
        "hyperf/event": "2.2.*@beta",
        "hyperf/model-listener": "2.2.*@beta",
        "hyperf/pool": "2.2.*@beta"
    },
    "suggest": {
        "nashgao/hyperf-mysql-spatial": "~0.1"
    },
    "require-dev": {
        "hyperf/di": "2.2.*@beta",
        "hyperf/contract": "2.2.*@beta",
        "hyperf/command": "2.2.*@beta",
        "hyperf/config": "2.2.*@beta",
        "hyperf/framework": "2.2.*@beta",
        "hyperf/dispatcher": "2.2.*@beta",
        "hyperf/server": "2.2.*@beta",
        "hyperf/logger": "2.2.*@beta",
        "hyperf/exception-handler": "2.2.*@beta",
        "swoole/ide-helper": "~4.6",
        "friendsofphp/php-cs-fixer": "^2.14",
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^0.12"
    },
    "autoload": {
        "psr-4": {
            "Nashgao\\MySQL\\QueryBuilder\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nashgao\\MySQL\\QueryBuilder\\Test\\": "./test"
        }
    },
    "scripts": {
        "test": "./vendor/bin/co-phpunit --colors=always",
        "test-coverage": "phpunit --colors=always --coverage-clover clover.xml",
        "license-check": "docheader check src/ tests/",
        "cs-fix": "php-cs-fixer fix $1",
        "json-fix": "./bin/composer-json-fixer",
        "analyse": "phpstan analyse --memory-limit 4096M -l 5 -c phpstan.neon ./src ./test"
    }
}
